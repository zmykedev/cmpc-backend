#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/savag/Nueva carpeta/cmpc-backend/node_modules/.pnpm/typeorm@0.3.26_pg@8.16.3_re_29c67c887cff74e4bc5923a7c3599064/node_modules/typeorm/node_modules:/mnt/c/Users/savag/Nueva carpeta/cmpc-backend/node_modules/.pnpm/typeorm@0.3.26_pg@8.16.3_re_29c67c887cff74e4bc5923a7c3599064/node_modules:/mnt/c/Users/savag/Nueva carpeta/cmpc-backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/savag/Nueva carpeta/cmpc-backend/node_modules/.pnpm/typeorm@0.3.26_pg@8.16.3_re_29c67c887cff74e4bc5923a7c3599064/node_modules/typeorm/node_modules:/mnt/c/Users/savag/Nueva carpeta/cmpc-backend/node_modules/.pnpm/typeorm@0.3.26_pg@8.16.3_re_29c67c887cff74e4bc5923a7c3599064/node_modules:/mnt/c/Users/savag/Nueva carpeta/cmpc-backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
fi
