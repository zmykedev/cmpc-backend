# Production Environment Variables for CMPC Backend
# Copy this file and rename to .env.production with your actual values

# Application
NODE_ENV=production
PORT=3000

# Database Configuration
DATABASE_HOST=your-production-db-host
DATABASE_PORT=5432
DATABASE_USER=your-production-db-user
DATABASE_PASSWORD=your-production-db-password
DATABASE_NAME=your-production-db-name

# JWT Configuration
JWT_SECRET=your-super-secure-jwt-secret-key-here

# Google Cloud Storage
GCS_PROJECT_ID=strategic-arc-471303-m4
GCS_BUCKET_NAME=cmpc-book
GCS_KEY_FILE=google-strategy-new.json

# CORS Configuration
CORS_ENABLED=true

# Logging
LOG_LEVEL=info
